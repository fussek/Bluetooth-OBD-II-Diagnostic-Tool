<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bluetoothterminal">

    <uses-permission android:name="android.permission.BLUETOOTH" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Bluetooth OBD-II Diagnostic Tool"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Mod_1.Mod1Activity" />
        <activity android:name=".Mod_2.Mod2Activity" />
        <activity android:name=".Mod_3.Mod3Activity" />
        <activity android:name=".Mod_4.Mod4Activity" />
        <activity android:name=".Mod_6.Mod6Activity" />
        <activity android:name=".TerminalActivity" />
        <activity android:name=".Mod_5.Mod5Activity" />
        <activity android:name=".Mod_7.Mod7Activity" />
        <activity android:name=".Mod_9.Mod9Activity"></activity>
    </application>

</manifest>